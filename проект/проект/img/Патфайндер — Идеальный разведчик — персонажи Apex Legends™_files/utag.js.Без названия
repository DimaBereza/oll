return action;};this.getLabel=function(detail){var ga_detail=detail.text?detail.text.trim():'';var ga_url=detail.url;return(ga_detail||'Null')+'__'+ga_url;};this.getCategoryFromUrl=function(url){var urls=window.tlm_data.url_categories;for(var key in urls){var re=new RegExp('('+key+')(\/|$)','i');var matches=re.exec(url);if(matches&&matches[1]){return urls[matches[1]];}}
